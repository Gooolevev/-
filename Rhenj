```python
def hello_world():
    print("–ü—Ä–∏–≤–µ—Ç, –º–∏—Ä! üåç")
hello_world()

import telebot
from telebot.types import ReplyKeyboardMarkup, KeyboardButton
import requests

bot = telebot.TeleBot('7990652825:AAHe9nvSi9nAnDweaLGXcxAILFh7OfrmxH0')  # –ó–∞–º–µ–Ω–∏ –Ω–∞ —Å–≤–æ–π —Ç–æ–∫–µ–Ω

@bot.message_handler(commands=['start'])
def send_welcome(message):
    markup = ReplyKeyboardMarkup(resize_keyboard=True)
    button1 = KeyboardButton("–ö–æ—Ç–∏–∫")
    button2 = KeyboardButton("–ü—Ä–∏–≤–µ—Ç")
    markup.add(button1, button2)
    bot.reply_to(message, "–ü—Ä–∏–≤–µ—Ç! –ù–∞–∂–º–∏ –∫–Ω–æ–ø–∫—É:", reply_markup=markup)

@bot.message_handler(func=lambda message: True)
def handle_message(message):
    if message.text == "–ö–æ—Ç–∏–∫":
        response = requests.get("https://api.thecatapi.com/v1/images/search")
        if response.status_code == 200:
            cat_url = response.json()[0]['url']
            bot.send_photo(message.chat.id, cat_url)
        else:
            bot.reply_to(message, "–ù–µ –º–æ–≥—É –Ω–∞–π—Ç–∏ –∫–æ—Ç–∏–∫–∞ :(")
    elif message.text == "–ü—Ä–∏–≤–µ—Ç":
        bot.reply_to(message, "–ü—Ä–∏–≤–µ—Ç, –∫–∞–∫ –¥–µ–ª–∞?")
    else:
        bot.reply_to(message, f"–¢—ã —Å–∫–∞–∑–∞–ª: {message.text}")

bot.polling()


